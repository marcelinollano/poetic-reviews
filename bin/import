#!/usr/bin/env ruby

require 'rubygems'
require 'bundler'
Bundler.require
Dotenv.load

Dir["./lib/*.rb"].each { |file| require file }

thing  = Thing.save({:name => "Hello Coffee", :url => "https://www.google.com/maps/place/Hola+Coffee"})
review = Review.save({:thing_id => thing.id, :text => "Es una gran cafeteria, me mola todo.", :author => "Marce"})
